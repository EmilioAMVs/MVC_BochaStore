@model Marca

@{
    ViewData["Title"] = "Marca Asociada";
}
    <br />
<h1>@ViewData["Title"]</h1>
    <br />
<form method="post">
    <div class="mb-3">
        <label>Id Marca: @Model.idMarca</label>
    </div>
    <div class="mb-3">
        <label>Nombre: @Model.nombreMarca</label>
    </div>
    <div class="row col-6">
        <div class="col-6 col-md-6">
            <br />
            <a asp-action="Index" class="btn btn-info">Regresar</a>
        </div>
    </div>
</form>

